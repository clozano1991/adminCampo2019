 <tr class="filaMiniListaContenida nested-fields " id="filaEmpleado_pago_<%=@empleado_pago.id%>">
    <%=f.text_field :haberDescuentoExtra,class: "form-control formEscondido", value: "descuento"%>
    <td class="columnaInformacionMiniListaContenidaCuatroPorFila">
        <div class="pocaInformacionMiniListaContenida">
            <div class="contenedorUnaInformacionPocaInformacionMiniListaContenida">
                <%=f.text_field :item, class: "form-control formSinBorde"%>
            </div> 
        </div>
    </td>
    <td class="columnaInformacionMiniListaContenidaCuatroPorFila">
        <div class="pocaInformacionMiniListaContenida">
            <div class="contenedorUnaInformacionPocaInformacionMiniListaContenida">
                <%=f.number_field :cantidad, class: "form-control formSinBorde inputCantidadItemPago", value: 1%>
            </div>
        </div> 
    </td>
    <td class="columnaInformacionMiniListaContenidaCuatroPorFila">
        <div class="pocaInformacionMiniListaContenida e">
            <div class="contenedorUnaInformacionPocaInformacionMiniListaContenida">
                <%=f.number_field :valor, class: "form-control formSinBorde inputValorItemPago", value: 0, step: :any%>
            </div>
        </div>
    </td>
    <td class="columnaInformacionMiniListaContenidaCuatroPorFila">
        <div class="pocaInformacionMiniListaContenida ">
            <div class="contenedorUnaInformacionPocaInformacionMiniListaContenida contenedorMonto">
                0
            </div>
        </div>
    </td>
    <td class="columnaImagenesDerechaMiniListaContenida">
        <div class="pocaInformacionMiniListaContenida " >
            <div class="contenedorImagenesYBotonesDerechaMiniListaContenida">
                <div class="imagenesDerechaMiniListaContenida">
                    <div class="imagenesDerechaMiniListaContenida">
                    <%=link_to_remove_association(image_tag("deleteRed.png",class: "fotoOpcion imagenDerechaBorrarItemPago"), f)%> 
                </div>
            </div>
        </div>
    </td>
</tr>